{
  "name": "@opentelemetry-bundler-plugins/tests",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "test": {
        "executor": "nx:run-commands",
        "outputs": [
          "{options.outputPath}"
        ],
        "dependsOn": [
          "^build"
        ],
        "options": {
          "cwd": "apps/opentelemetry-plugins-node-tests",
          "command": "nyc node --require ts-node/register --test 'test/**/*.test.ts'",
          "outputPath": "packages/opentelemetry-plugins-node-tests/test-dist"
        }
      }
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.9",
    "@fastify/rate-limit": "^10.2.2",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.58.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@types/node": "^22.15.2",
    "@types/webpack": "^5.28.5",
    "babel-loader": "^10.0.0",
    "esbuild": "^0.25.3",
    "fastify": "5.3.2",
    "graphql": "^16.11.0",
    "mongodb": "6.8.0",
    "mongodb-memory-server": "9.5.0",
    "nyc": "17.1.0",
    "pino": "^9.6.0",
    "prettier": "3.5.3",
    "redis": "^4.7.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3",
    "webpack": "^5.99.7"
  }
}
