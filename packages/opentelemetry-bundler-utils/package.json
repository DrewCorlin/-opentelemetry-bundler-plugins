{
  "name": "@opentelemetry-bundler-plugins/opentelemetry-bundler-utils",
  "version": "0.3.1-6",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "nx": {
    "targets": {
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "packages/opentelemetry-bundler-utils",
          "command": "rm -rf dist"
        }
      },
      "publish": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "packages/opentelemetry-bundler-utils",
          "command": ":"
        }
      }
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "dependencies": {
    "@opentelemetry/instrumentation": "^0.200.0",
    "semver": "^7.7.1",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/semver": "^7.7.0"
  }
}
