{
  "name": "@opentelemetry-bundler-plugins/opentelemetry-esbuild-plugin-node",
  "version": "0.0.1",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "nx": {
    "targets": {
      "clean": {
        "executor": "nx:run-commands",
        "options": {
          "cwd": "packages/opentelemetry-esbuild-plugin-node",
          "command": "rm -rf dist"
        }
      }
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@types/node": "^22.14.1",
    "@types/semver": "^7.7.0",
    "ts-node": "^10.9.2",
    "typescript": "5.8.3"
  },
  "dependencies": {
    "tslib": "^2.3.0",
    "@opentelemetry-bundler-plugins/opentelemetry-bundler-utils": "0.0.1"
  },
  "peerDependencies": {
    "esbuild": ">=0.19.x <=0.25.x"
  }
}
